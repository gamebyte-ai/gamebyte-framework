{
  "name": "@gamebyte/framework",
  "type": "module",
  "version": "1.3.0",
  "description": "A comprehensive JavaScript game engine framework that unifies 2D and 3D game development with Laravel-inspired architecture",
  "main": "dist/index.js",
  "module": "dist/index.js",
  "types": "dist/index.d.ts",
  "sideEffects": false,
  "exports": {
    ".": {
      "import": "./dist/index.js",
      "require": "./dist/index.cjs.js",
      "types": "./dist/index.d.ts"
    },
    "./core": {
      "import": "./dist/core/index.js",
      "types": "./dist/core/index.d.ts"
    },
    "./rendering": {
      "import": "./dist/rendering/index.js",
      "types": "./dist/rendering/index.d.ts"
    },
    "./ui": {
      "import": "./dist/ui/index.js",
      "types": "./dist/ui/index.d.ts"
    },
    "./ui/components": {
      "import": "./dist/ui/components/index.js",
      "types": "./dist/ui/components/index.d.ts"
    },
    "./ui/screens": {
      "import": "./dist/ui/screens/index.js",
      "types": "./dist/ui/screens/index.d.ts"
    },
    "./ui/panels": {
      "import": "./dist/ui/panels/index.js",
      "types": "./dist/ui/panels/index.d.ts"
    },
    "./ui/effects": {
      "import": "./dist/ui/effects/index.js",
      "types": "./dist/ui/effects/index.d.ts"
    },
    "./ui/themes": {
      "import": "./dist/ui/themes/index.js",
      "types": "./dist/ui/themes/index.d.ts"
    },
    "./audio": {
      "import": "./dist/audio/index.js",
      "types": "./dist/audio/index.d.ts"
    },
    "./input": {
      "import": "./dist/input/index.js",
      "types": "./dist/input/index.d.ts"
    },
    "./physics": {
      "import": "./dist/physics/index.js",
      "types": "./dist/physics/index.d.ts"
    },
    "./physics/2d": {
      "import": "./dist/physics/matter2d.js",
      "types": "./dist/physics/matter2d.d.ts"
    },
    "./physics/3d": {
      "import": "./dist/physics/cannon3d.js",
      "types": "./dist/physics/cannon3d.d.ts"
    },
    "./graphics": {
      "import": "./dist/graphics/index.js",
      "types": "./dist/graphics/index.d.ts"
    },
    "./scenes": {
      "import": "./dist/scenes/index.js",
      "types": "./dist/scenes/index.d.ts"
    },
    "./layout": {
      "import": "./dist/layout/index.js",
      "types": "./dist/layout/index.d.ts"
    },
    "./facades": {
      "import": "./dist/facades/index.js",
      "types": "./dist/facades/index.d.ts"
    },
    "./contracts": {
      "import": "./dist/contracts/index.js",
      "types": "./dist/contracts/index.d.ts"
    },
    "./utils": {
      "import": "./dist/utils/index.js",
      "types": "./dist/utils/index.d.ts"
    },
    "./performance": {
      "import": "./dist/performance/index.js",
      "types": "./dist/performance/index.d.ts"
    },
    "./three-toolkit": {
      "import": "./dist/three/index.js",
      "types": "./dist/three/index.d.ts"
    },
    "./three/lights": {
      "import": "./dist/three/lights/index.js",
      "types": "./dist/three/lights/index.d.ts"
    },
    "./three/loaders": {
      "import": "./dist/three/loaders/index.js",
      "types": "./dist/three/loaders/index.d.ts"
    },
    "./three/animation": {
      "import": "./dist/three/animation/index.js",
      "types": "./dist/three/animation/index.d.ts"
    },
    "./merge": {
      "import": "./dist/merge/index.js",
      "types": "./dist/merge/index.d.ts"
    },
    "./tick": {
      "import": "./dist/tick/index.js",
      "types": "./dist/tick/index.d.ts"
    },
    "./resources": {
      "import": "./dist/resources/index.js",
      "types": "./dist/resources/index.d.ts"
    },
    "./three/events": {
      "import": "./dist/three/events/index.js",
      "types": "./dist/three/events/index.d.ts"
    },
    "./three/instancing": {
      "import": "./dist/three/instancing/index.js",
      "types": "./dist/three/instancing/index.d.ts"
    },
    "./postprocessing": {
      "import": "./dist/postprocessing/index.js",
      "types": "./dist/postprocessing/index.d.ts"
    },
    "./environment": {
      "import": "./dist/environment/index.js",
      "types": "./dist/environment/index.d.ts"
    },
    "./prefabs": {
      "import": "./dist/prefabs/index.js",
      "types": "./dist/prefabs/index.d.ts"
    },
    "./assets/pipeline": {
      "import": "./dist/assets/pipeline-index.js",
      "types": "./dist/assets/pipeline-index.d.ts"
    }
  },
  "scripts": {
    "build": "tsc && rollup -c && rollup -c rollup.umd.config.js",
    "dev": "tsc --watch",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:coverage": "jest --coverage",
    "test:integration": "jest --testPathPattern=integration",
    "test:unit": "jest --testPathPattern=__tests__ --testPathIgnorePatterns=integration",
    "lint": "eslint src/**/*.ts",
    "format": "prettier --write src/**/*.ts",
    "clean": "rimraf dist",
    "prepublishOnly": "npm run clean && npm run build",
    "validate:agent-docs": "bash scripts/validate-agent-docs.sh",
    "docs:agent": "echo 'Agent docs: docs/agent-guide/CORE_API.md'",
    "examples:list": "find examples/ -name '*.html' -type f",
    "release:patch": "npm version patch && git push && git push --tags",
    "release:minor": "npm version minor && git push && git push --tags",
    "release:major": "npm version major && git push && git push --tags",
    "release:tag": "git tag -a v$(node -p \"require('./package.json').version\") -m \"Release v$(node -p \"require('./package.json').version\")\" && git push --tags"
  },
  "keywords": [
    "game-engine",
    "javascript",
    "typescript",
    "pixi.js",
    "three.js",
    "mobile-games",
    "2d",
    "3d",
    "framework",
    "tree-shaking"
  ],
  "author": "GameByte Team",
  "license": "MIT",
  "dependencies": {
    "@pixi/layout": "^3.0.0",
    "@pixi/ui": "^2.3.2",
    "cannon-es": ">=0.20.0",
    "eventemitter3": "^5.0.1",
    "matter-js": ">=0.19.0",
    "pixi.js": "^8.15.0",
    "reflect-metadata": "^0.2.1",
    "three": ">=0.150.0"
  },
  "devDependencies": {
    "@babel/core": "^7.28.4",
    "@babel/preset-env": "^7.28.3",
    "@eslint/js": "^9.39.3",
    "@rollup/plugin-commonjs": "^25.0.7",
    "@rollup/plugin-node-resolve": "^15.2.3",
    "@rollup/plugin-wasm": "^6.2.2",
    "@types/jest": "^29.5.14",
    "@types/matter-js": "^0.19.0",
    "@types/three": "^0.169.0",
    "babel-jest": "^30.2.0",
    "eslint": "^9.39.3",
    "http-server": "^14.1.1",
    "jest": "^29.7.0",
    "jest-environment-jsdom": "^29.7.0",
    "prettier": "^3.2.4",
    "rimraf": "^5.0.5",
    "rollup": "^4.9.6",
    "rollup-plugin-typescript2": "^0.36.0",
    "ts-jest": "^29.4.0",
    "typescript": "^5.3.3",
    "typescript-eslint": "^8.56.1"
  },
  "peerDependencies": {
    "@pixi/layout": "^3.0.0",
    "@pixi/ui": "^2.3.2",
    "cannon-es": ">=0.20.0",
    "matter-js": ">=0.19.0",
    "pixi.js": "^8.15.0",
    "postprocessing": ">=6.30.0",
    "three": ">=0.150.0"
  },
  "peerDependenciesMeta": {
    "three": {
      "optional": true
    },
    "cannon-es": {
      "optional": true
    },
    "matter-js": {
      "optional": true
    },
    "postprocessing": {
      "optional": true
    }
  },
  "files": [
    "dist/**/*",
    "README.md",
    "LICENSE"
  ],
  "repository": {
    "type": "git",
    "url": "https://github.com/gamebyte/framework.git"
  },
  "engines": {
    "node": ">=16.0.0"
  }
}
